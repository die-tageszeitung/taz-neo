<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CoverView">
        <attr name="archive_item_padding" format="dimension" />
        <attr name="archive_item_text_color" format="color" />
        <attr name="archive_item_text_orientation">
            <flag name="inherit" value="0" />
            <flag name="gravity" value="1" />
            <flag name="text_start" value="2" />
            <flag name="text_end" value="3" />
            <flag name="center" value="4" />
            <flag name="view_start" value="5" />
            <flag name="view_end" value="6" />
        </attr>
        <attr name="do_not_show_download_icon" format="boolean" />

    </declare-styleable>
    <declare-styleable name="DottedLineView">
        <attr name="color" format="color" />
    </declare-styleable>
    <integer name="text_default_size">100</integer>
    <fraction name="nav_button_scale_factor">33%</fraction>
</resources>