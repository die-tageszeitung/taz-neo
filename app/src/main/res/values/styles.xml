    <style name="TextAppearance.Date.Small" parent="TextAppearance">
        <item name="android:fontFamily">@font/appFontOldRegular</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:lineSpacingExtra">5sp</item>
    </style>

    <style name="TextAppearance.CoachMark.Header" parent="TextAppearance">
        <item name="android:fontFamily">@font/appFontCoachMarkHeader</item>
        <item name="android:textSize">32sp</item>
        <item name="android:lineHeight" tools:targetApi="p">@dimen/coach_mark_header_line_height</item>
